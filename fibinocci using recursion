#include <stdio.h>
int fib(int n1)
{ 
    if(n1==0)
    {
        return 0;
    }
  else if(n1==1)
    {
        return 1;
    }
    else
    {
        return fib(n1-1)+fib(n1-2);
    }
    }

int main()
{
    int n;
    printf("enter the value of n:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        printf("%d ",fib(i));
    }
}
